{
  "name": "react-portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/imagemin": "^8.0.0",
    "@types/imagemin-jpegtran": "^5.0.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "imagemin": "^8.0.1",
    "mobx": "^6.7.0",
    "mobx-persist-store": "^1.1.2",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fade-in": "^2.0.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "use-breakpoint": "^3.0.6",
    "web-vitals": "^3.1.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "node build-blog.mjs && REACT_APP_BUILD_NAME=$(git rev-parse --short HEAD) craco build && node postbuild.mjs",
    "test": "craco test",
    "eject": "react-scripts eject",
    "new:blog": "node new-blog.mjs",
    "postinstall": "node build-blog.mjs",
    "watch": "concurrently -n \"blog,app\" \"npm run watch:blog\" \"npm start\"",
    "watch:blog": "chokidar \"blogs/**\" -c \"node build-blog.mjs\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "nth-check": "2.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/prompts": "^2.4.4",
    "chokidar-cli": "^3.0.0",
    "craco-esbuild": "^0.5.2",
    "esbuild": "^0.16.3",
    "glob": "^10.2.6",
    "handlebars": "^4.7.7",
    "highlight.js": "^11.8.0",
    "marked": "^5.0.2",
    "marked-highlight": "^2.0.0",
    "prompts": "^2.4.2"
  }
}
